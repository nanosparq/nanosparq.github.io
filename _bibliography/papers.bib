@article{ WOS:000589902500001,
Author = {Morozovska, Anna N. and Eliseev, Eugene A. and Kelley, Kyle and
   Vysochanskii, Yulian M. and Kalinin, V, Sergei and Maksymovych, Petro},
Title = {Phenomenological description of bright domain walls in
   ferroelectric-antiferroelectric layered chalcogenides},
Journal = {PHYSICAL REVIEW B},
Year = {2020},
Volume = {102},
Number = {17},
Month = {NOV 17},
Abstract = {Recently, a layered ferroelectric CuInP2Se6 was shown to exhibit domain
   walls with locally enhanced piezoresponse-a striking departure from the
   observations of nominally zero piezoresponse in most ferroelectrics.
   Although it was proposed that such ``bright{''} domain walls are phase
   boundaries between fern- and antiferroelectrically ordered regions of
   the materials, the physical mechanisms behind the existence and response
   of these boundaries remain to be understood. Here, using
   Landau-Ginzburg-Devonshire phenomenology combined with a four
   sublattices model, we describe quantitatively the bright-contrast and
   dark-contrast domain boundaries between the antiferroelectric,
   ferroelectric, or ferrielectric long-range-ordered phases in a layered
   ferroelectric-antiferroelectric ferroics, such as CuInP2(S1-ySey)(6).},
DOI = {10.1103/PhysRevB.102.174108},
Article-Number = {174108},
